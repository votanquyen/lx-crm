{
  "name": "locxanh-v4",
  "version": "4.0.0",
  "private": true,
  "packageManager": "pnpm@10.26.0",
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx && tsc --noEmit",
    "lint:fix": "eslint . --ext .ts,.tsx --fix && prettier --write .",
    "format": "prettier --write .",
    "validate": "tsc --noEmit && eslint . --ext .ts,.tsx",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev --config prisma/prisma.config.ts",
    "db:migrate:prod": "prisma migrate deploy --config prisma/prisma.config.ts",
    "db:migrate:status": "prisma migrate status --config prisma/prisma.config.ts",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:realdb": "tsx prisma/seed-realdb.ts --preset=realdb",
    "db:studio": "prisma studio",
    "prepare": "husky"
  },
